  <div class="login-section">
    <div class="login-section-bg"></div>
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <div class="login-box ">
            <form method="post" action="/">
                
                {% if errorText %}
        <div class="alert alert-dismissible alert-danger">
          <button type="button" class="close" data-dismiss="alert">×</button>
          {{ errorText }}
        </div>
      {% endif %}
      {% if successText %}
        <div class="alert alert-dismissible alert-success">
          <button type="button" class="close" data-dismiss="alert">×</button>
          {{ successText }}
        </div>
      {% endif %}
                
              <div class="login-box-item">
                <input type="text" class="login-label" id="username" name="username" value="" placeholder=" {{ lang["signin.username"] }}">
              </div>
              <div class="login-box-item">
                <input type="password" class="login-label" id="password" name="password" value="" placeholder="Password"><a href="/resetpassword">Esqueci Minha Senha</a></span>
              </div>
                            <div class="login-box-item checkbox-hidden">
                <input type="checkbox"  name="LoginForm[remember]" value="1" id="remember">
                <label class="login-remember-box" for="remember">
                  <i class="fa fa-bookmark"></i>
                </label>
               
                <button type="submit" class="btn btn-blue login-btn"> {{ lang["signin.title"] }}</button>
                
                    {% if google == 2 %}
    <a href="/google" class="btn btn-blue"><i class="fab fa-google"></i>&nbsp;Google Login</a>
{% endif %}
              </div>
              
             
              
            </form>
          </div>
        </div>
      </div>
    </div>
